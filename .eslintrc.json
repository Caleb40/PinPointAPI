{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "parser": "@babel/eslint-parser",
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "one-var": [
      "off"
    ]
  }
}